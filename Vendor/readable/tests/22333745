<script>
(function(){
  var menuBar = document.getElementById('Wretch-haha-menubar');
  var query = document.getElementById('Wretch-haha-query-text');
  if(query){
    query.onfocus = function() {
      if(this.value == '找相簿' || this.value == '找文章' || this.value == '找影音' || this.value == '找朋友' || this.value == '找揪團') 
        this.value = '';
    }
  }
})();
</script>
<script src="http://yui.yahooapis.com/combo?3.3.0/build/yui/yui-min.js"></script>
<script>
/*for vitality*/
YUI().use('node', 'event', 'event-delegate', 'io', 'json-parse', function(Y){
    var crumb = Y.one('#crumb').get('value'),
        user = Y.one('#login').get('value'),
        bAskVitality = (Y.one('#ask-vitality').get('value')==='1')? true: false,
        bNewVitality = (Y.one('#new-vitality').get('value')==='1')? true: false,
        user = Y.one('#login').get('value'),
        vitalityList = Y.one('#Wretch-haha-vitality-list'),
        vitalityBtn = null,
        bodyWrap = Y.one('body'),
        bReadVitality = false,
        bAbort = false,
        bReading = false,
        bRequesting = false,
        hahaBar = Y.one('#Wretch-haha'),
        loading = Y.one('.vitality-loading').cloneNode(true);
    var fixIESelect = function () {
        if (Y.UA.ie) {
            if (vitalityList.hasClass('expanded')) {
                Y.all('select').addClass('hide-for-ie');
            } else {
                Y.all('select').removeClass('hide-for-ie');
            }
        }
        return;
    };
    var expandList = function (e) {
        bReadVitality = true;
        if (bAbort===true) { // close
            vitalityList.removeClass('expanded');
            vitalityList.removeClass('loading');
            fixIESelect();
            return;
        }
        if(!vitalityList.one('.vitality-loading')){
            vitalityList.insertBefore(loading.cloneNode(true), Y.one('.vitality-show-all'));
        }
        if (e.type === 'click'||e.type === 'focus') {
            if (vitalityList.hasClass('expanded')) {
                vitalityList.removeClass('expanded');
                vitalityList.removeClass('loading');
                bReading = false;
            } else {
                vitalityList.addClass('expanded');
                vitalityList.addClass('loading');
                update(e);
            }            
        } else {
            vitalityList.addClass('expanded');
            vitalityList.addClass('loading');
            if (Y.UA.ie) {
                Y.all('select').addClass('hide-for-ie');
            }
            later(update, e);
        } 
        fixIESelect();
    };
    var emptyList = function () {
        if(vitalityList.one('.vitality-show-all')) {
            vitalityList.one('.vitality-show-all').remove();
        }
    };
    var update = function (e) {
        e.currentTarget.removeClass('vitality-alert');
        var uri = '/ajax/common/ajax_get_vitality_updates.php',
            request,
            onCompelete = function(id, o, args) {
                try {
                    emptyList();
                    vitalityList.one('ul').set('innerHTML', o.responseText);
                } catch (e) {
                    vitalityList.set('innerHTML', o.responseText);
                }
                vitalityList.removeClass('loading');
                bReading = true;
                bAbort = false;
                bRequesting = false;
                fixIESelect();
            },
            cfg = {method: 'GET', data:'crumb='+crumb, on:{complete:onCompelete}};
            if (!bRequesting) {
                bRequesting = true;
                request = Y.io(uri, cfg);
            }
    };
    var later = function (fn, arg) {
        setTimeout(function(){
            if(!bAbort) {
                fn(arg);
            } else {
                vitalityList.removeClass('expanded');
                fixIESelect();
            }
        }, 1000);
    };
    var hover = function (eT) {
        eT.toggleClass('on');
        if(Y.UA.ie < 7 && eT.get('id') === 'Wretch-haha-vitality') {
           eT.toggleClass('btn-on'); 
        }
    };
    var resize = function () {
        if(user === '1') {
            vitalityBtn = Y.one('#Wretch-haha-vitality');
            var x = vitalityBtn.getX()+'px';
            vitalityList.setStyle('left', x);
            if (Y.UA.ie === 6){
                vitalityList.setStyle('marginLeft', '-220px');
            } else {
                vitalityList.setStyle('marginLeft', '-220px');
            }
        }
    };
    var init = function () {
    if (user === '0') {
        bReadVitality = true;
        Y.one('#Wretch-haha-menubar').on('mouseover', function(e){
            vitalityList.removeClass('expanded');
            bAbort = true;
            bReading = false;
            e.currentTarget.one('span').setStyle('display', 'block');
        });
        Y.one('#Wretch-haha-menubar').on('mouseleave', function(e){
            e.currentTarget.one('span').setStyle('display', 'none');
        });
    } else if (user === '1') {
        vitalityBtn = Y.one('#Wretch-haha-vitality'); 
        Y.one('#Wretch-haha-menubar').on('mouseover', function(e){
            vitalityList.removeClass('expanded');
            bAbort = true;
            bReading = false;
            e.currentTarget.one('span').setStyle('display', 'block');
        });
        Y.one('#Wretch-haha-menubar').on('mouseleave', function(e){
            e.currentTarget.one('span').setStyle('display', 'none');
        });
        vitalityBtn.on('click', function(e){
            hover(e.currentTarget);
            e.halt();
            bAbort = false;
            expandList(e);
        });
        vitalityBtn.on('mouseover', function(e){
            hover(e.currentTarget);
            e.halt();
            bAbort = false;
            if (!bReading) { //not compeleted display
                expandList(e);
            }
        });
        vitalityBtn.on('mouseout', function(e){
            hover(e.currentTarget);
            e.halt();
            bAbort = true;
            if (!bReading) { //not compeleted display
                expandList(e);
            }
        });
        try {
        vitalityBtn.on('focus', function(e){
            e.halt();
            bAbort = false;
            expandList(e);
        });
        vitalityBtn.on('blur', function(e){
            e.halt();
            bAbort = true;
            expandList(e);
        });
        } catch (e) {}
        Y.delegate('mouseover', function(e) {
            hover(this);
        }, '#Wretch-haha-vitality-list', '.vitality-msg');
        Y.delegate('mouseout', function(e) {
            hover(this);
        }, '#Wretch-haha-vitality-list', '.vitality-msg');
        Y.delegate('click', function(e) {
            location.href = (e.currentTarget.one('a').getAttribute('href'));
        }, '#Wretch-haha-vitality-list', '.vitality-msg');
        Y.delegate('mouseover', function(e) {
            hover(this);
        }, '#Wretch-haha-vitality-list', 'ul li');
        Y.delegate('mouseout', function(e) {
            hover(this);
        }, '#Wretch-haha-vitality-list', 'ul li');
        Y.delegate('click', function(e) {
            location.href = this.getAttribute('_href');
        }, '#Wretch-haha-vitality-list', 'ul li');
        }
    };

    Y.on('resize', function(){resize();});
    Y.one('body').on('click', function(){
        vitalityList.removeClass('expanded');
        fixIESelect();
        bAbort = true;
        bReading = false;
    });
    Y.on('domready', function (){
        resize();
        init();
        setTimeout(function(){
        var uri = '/ajax/common/ajax_has_new_vitality.php',
            onCompelete = function(id, o, args) {
                var oJSON = Y.JSON.parse(o.responseText);
                if (oJSON.result) {
                    Y.one('#Wretch-haha-vitality').addClass('vitality-alert');
                }
            },
            cfg = {method: 'GET', data:'crumb='+crumb, on:{complete:onCompelete}};
            if (!bReadVitality&bAskVitality) {
                request = Y.io(uri, cfg);
            } else if (!bReadVitality&&!bAskVitality&&bNewVitality) {
                Y.one('#Wretch-haha-vitality').addClass('vitality-alert');
            }
        }, 5000);
    });
});
</script>
